

TERM:
    LITERAL
    identifier
    COMPOUND
    new BROAD_TYPE ( ARGS_LIST )

COMPOUND:
    identifier
    ARRAY_ACCESS
    FUNCTION_CALL
    identifier . COMPOUND
    COMPOUND . COMPOUND

ARRAY_ACCESS:
    COMPOUND [ EXPRESSION ]

FUNCTION_CALL:
    identifier ( ARG_LIST )
    COMPOUND . identifier (ARG_LIST)

ARGS_LIST:
    \0
    EXPRESSION
    EXPRESSION , ARG_LIST


